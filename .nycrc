{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "branches": 75,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "includes": [
    "src/**/*"
  ],
  "exclude": [
    "src/app/api/coverage/route.ts"
  ],
  "temp-dir": "./coverage",
  "reporter": [
    "lcov",
    "text-summary"
  ]
}